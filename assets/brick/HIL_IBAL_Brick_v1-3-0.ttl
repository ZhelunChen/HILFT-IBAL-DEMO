# ICE TANK
IBAL:icetank a brick:HVAC_Equipment ;
	brick:isFedBy IBAL:pump2,
		IBAL:ahu1,
		IBAL:ahu2 ;
	brick:hasPoint IBAL:ts_mode,
		IBAL:ts_meter,
		IBAL:ts_f,
		IBAL:ts_in_rtd,
		IBAL:ts_out_rtd .

IBAL:ts_mode a brick:Status ;
	brick:hasUnit unit:UNITLESS ;
	rdfs:comment "Ice tank mode (0 - off, 1 - discharge, 2 - charge)" .
IBAL:ts_meter a brick:Sensor ;
	brick:hasUnit unit:PERCENT .
IBAL:ts_f a brick:Water_Flow_Sensor ;
	brick:hasUnit unit:GAL_US-PER-MIN .
IBAL:ts_in_rtd a brick:Entering_Chilled_Water_Temperature_Sensor ;
	brick:hasUnit unit:DEG_F .
IBAL:ts_out_rtd a brick:Ice_Tank_Leaving_Water_Temperature_Sensor ;
	brick:hasUnit unit:DEG_F .

# PUMP 1
IBAL:pump1 a brick:Chilled_Water_Pump ;
	brick:isFedBy IBAL:chiller1 ;
	brick:hasPoint IBAL:pump1_power .

IBAL:pump1_power a brick:Electric_Power_Sensor ;
	brick:hasUnit unit:W .

# PUMP 2
IBAL:pump2 a brick:Chilled_Water_Pump ;
