# CHILLER 1
IBAL:chiller1 a brick:Chiller ;
	brick:isFedBy IBAL:pump4 ;
	brick:hasPoint IBAL:ch1_power,
		IBAL:ch1_on	,
		IBAL:ch1_f_e,
		IBAL:ch1_e_in_rtd,
		IBAL:ch1_e_out_rtd,
		IBAL:ch1_t_sp_f,
		IBAL:ch1_f_c,
		IBAL:ch1_c_out_rtd .

IBAL:ch1_power a brick:Electric_Power_Sensor ;
	brick:hasUnit unit:W .
IBAL:ch1_on a brick:On_Off_Status .
IBAL:ch1_f_e a brick:Chilled_Water_Flow_Sensor ;
	brick:hasUnit unit:GAL_US-PER-MIN .
IBAL:ch1_e_in_rtd a brick:Entering_Chilled_Water_Temperature_Sensor ;
	brick:hasUnit unit:DEG_F .
IBAL:ch1_e_out_rtd a brick:Leaving_Chilled_Water_Temperature_Sensor ;
	brick:hasUnit unit:DEG_F .
IBAL:ch1_t_sp_f a brick:Chilled_Water_Temperature_Setpoint ;
	brick:hasUnit unit:DEG_F .
