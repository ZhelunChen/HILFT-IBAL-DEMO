	brick:hasPoint IBAL:pump2_power .

IBAL:pump2_power a brick:Electric_Power_Sensor ;
	brick:hasUnit unit:W .

# PUMP 3
IBAL:pump3 a brick:Water_Pump ;
	brick:isFedBy IBAL:chiller1,
		IBAL:chiller2,
		IBAL:icetank ;
	brick:hasPoint IBAL:pump3_power,
		IBAL:pump3_dp,
		IBAL:pump3_out_rtd,
		IBAL:sl_vfd_sp_v,
		IBAL:sl_f .

IBAL:pump3_power a brick:Electric_Power_Sensor ;
	brick:hasUnit unit:W .
IBAL:pump3_dp a brick:Chilled_Water_Differential_Pressure_Sensor ;
	brick:hasUnit unit:PSI .
IBAL:pump3_out_rtd a brick:Chilled_Water_Temperature_Sensor ;
	brick:hasUnit unit:DEG_F .
IBAL:sl_vfd_sp_v a brick:Chilled_Water_Differential_Pressure_Setpoint ;
	brick:hasUnit unit:PSI .
IBAL:sl_f a brick:Chilled_Water_Flow_Sensor ;
	brick:hasUnit unit:GAL_US-PER-MIN .

# PUMP 4
