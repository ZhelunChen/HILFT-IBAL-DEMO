# GRID SIGNALS
IBAL:grid_signal_type a brick:Status ;
	brick:hasUnit unit:UNITLESS ;
	rdfs:comment "Grid signal type (0 no signal, 1 shedding request, 2 time-of-use price, 3 mod)" .
IBAL:grid_signal_shed a brick:Load_Shed_Status ;
	brick:hasUnit unit:UNITLESS ;
	rdfs:comment "Grid signal shedding request (0 - no request, 1 - request)" .
IBAL:grid_signal_price a brick:Status ; 
	rdf:label "unit: USD-PER-KiloW-HR" .

# OUTDOOR EMULATOR
IBAL:out_emulator a brick:HVAC_Equipment ;
	brick:hasPoint IBAL:oau_t_out_sp_f,
		IBAL:oau_rh_out_sp_per .

IBAL:oau_t_out_sp_f a brick:Temperature_Setpoint ;
	brick:hasUnit unit:DEG_F .
IBAL:oau_rh_out_sp_per a brick:Humidity_Setpoint ;
	brick:hasUnit unit:KiloGM-PER-KiloGM .

